{
	"name": "DF_Generate_Surrogatekeys",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_employee",
						"type": "DatasetReference"
					},
					"name": "sourceEmployee"
				},
				{
					"dataset": {
						"referenceName": "ds_allEmployee",
						"type": "DatasetReference"
					},
					"name": "allEmployeewithData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "join2"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Empname as string,",
				"          gender as string,",
				"          Department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceEmployee",
				"source(output(",
				"          id as string,",
				"          Empname as string,",
				"          gender as string,",
				"          Department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> allEmployeewithData",
				"sourceEmployee keyGenerate(output(id as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"allEmployeewithData derive(Dummy = \"Dummy\") ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(Dummy),",
				"     Max_id = max(id)) ~> aggregate1",
				"surrogateKey1, aggregate1 join(1 == 1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 derive(New_id = toString(toInteger(Max_id) + toInteger(id))\r",
				") ~> derivedColumn2",
				"derivedColumn2 select(mapColumn(",
				"          id = New_id,",
				"          Empname,",
				"          gender,",
				"          Department",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}