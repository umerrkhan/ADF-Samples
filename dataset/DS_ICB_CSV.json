{
	"name": "DS_ICB_CSV",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat('Test_', convertFromUtc(utcnow(), 'Pacific Standard Time'),'.csv')",
					"type": "Expression"
				},
				"folderPath": "Target_dir",
				"container": "samplesfiles"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "id",
				"type": "String"
			},
			{
				"name": "email",
				"type": "String"
			},
			{
				"name": "first_name",
				"type": "String"
			},
			{
				"name": "last_name",
				"type": "String"
			},
			{
				"name": "avatar",
				"type": "String"
			}
		]
	}
}